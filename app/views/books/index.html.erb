<h2>Libraries Index</h2>

<ul class="list-group">
  <% @libraries.each do |library| %>
    <li class="list-group-item tab-pane"><%= link_to library.library_name, library_path(library), :class => "" %></li>
  <% end %>
</ul>


<h2>Books Index</h2>

<ul class="list-group">
  <% @books.each do |book| %>
    <li class="list-group-item tab-pane"><%= link_to book.title, book_path(book), :class => "" %></li>
  <% end %>
</ul>
